# MongoDB connection string
MONGO_URI=mongodb+srv://<username>:<password>@<cluster>.mongodb.net/movie_app?retryWrites=true&w=majority

# JWT secret key for signing tokens
JWT_SECRET=your_jwt_secret_here

# TMDB API key (you can obtain one from https://www.themoviedb.org/settings/api)
TMDB_API_KEY=your_tmdb_api_key_here

# Optional: Define port (default is 5000)
PORT=5000
